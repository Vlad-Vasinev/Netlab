          .nbl-project__img
            <video class="" loop muted="muted" autoplay="autoplay" playsinline webkit-playsinline preload="auto" poster="img/new/escape2.png">
              <source src="img/new/escape.webm" type="video/webm">
              <source src="img/new/escape.mp4" type="video/mp4">
            </video>
            .nbl-project__mesh 
              .nbl-project__icons 
                include ../nbl-projectIcon/_nbl-projectIconSmall
                include ../nbl-projectIcon/_nbl-projectIcon
          .nbl-project__info 
            .h3.nbl-textAnimation DoBuy shop
            .h7 development / support / creatives
