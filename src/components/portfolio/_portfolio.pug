        .nbl-linksSm__item 
          a.h2.nbl-textAnimation(href="#") Content creation
      .nbl-projects__wrapper
        .nbl-project(js-modal-open data-modal-name='caseModal' data-modal-url='api/case2.xml'  data-modal-title="Linktech app" data-modal-background='#F2F2F2')
          .nbl-project__img
            img(src="img/new/nbl-project.jpg" alt="")
            .nbl-project__mesh 
              .nbl-project__icons 
                include ../nbl-projectIcon/_nbl-projectIcon
                include ../nbl-projectIcon/_nbl-projectIconSmall
